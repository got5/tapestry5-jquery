        <classes>
          <class name="org.got5.tapestry5.jquery.AjaxFormLoopTest"></class>
          <class name="org.got5.tapestry5.jquery.BindTest"></class>
          <class name="org.got5.tapestry5.jquery.CarouselTest"></class>
