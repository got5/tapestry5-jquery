<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="tapestry5-jquery Application Test Suite" annotations="1.5">
   <test name="JQuery Integration Tests"  enabled="true">
    
        <parameter name="tapestry.web-app-folder" value="src/test/jquery/webapp"/>
        <parameter name="tapestry.browser-start-command" value="${browserStartCommand}" />

        <classes>
       	  <class name="org.got5.tapestry5.jquery.test.AjaxFormLoopTest"></class>
          <class name="org.got5.tapestry5.jquery.test.BindTest"></class>
          <class name="org.got5.tapestry5.jquery.test.CarouselTest"></class>
          <class name="org.got5.tapestry5.jquery.test.CheckboxTest"></class> 
          <class name="org.got5.tapestry5.jquery.test.CustomDatepickerTest"></class> 
          <class name="org.got5.tapestry5.jquery.test.DialogTest"></class>
          <class name="org.got5.tapestry5.jquery.test.JQueryTest"></class>
          <class name="org.got5.tapestry5.jquery.test.MaskTest"></class>
          <class name="org.got5.tapestry5.jquery.test.RevealTest"></class>
          <class name="org.got5.tapestry5.jquery.test.SelectorTest"></class>
          <class name="org.got5.tapestry5.jquery.test.SliderTest"></class>
          <class name="org.got5.tapestry5.jquery.test.SuperfishTest"></class>
          <class name="org.got5.tapestry5.jquery.test.TooltipTest"></class> 
          <class name="org.got5.tapestry5.jquery.test.GalleryTest"></class>
        </classes>
    </test>
</suite>
