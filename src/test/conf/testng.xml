          <class name="org.got5.tapestry5.jquery.LinksubmitTest"></class>
          <class name="org.got5.tapestry5.jquery.ZoneTest"></class>
          <class name="org.got5.tapestry5.jquery.DraggableTest"></class>
          <class name="org.got5.tapestry5.jquery.DataTableTest"></class>
        </classes>
    </test>
    <test name="JQuery Unit Tests"  enabled="true">
    	<classes>
       	  <class name="org.got5.tapestry5.jquery.unit.ImportJQueryUITest"></class>
       	  <class name="org.got5.tapestry5.jquery.unit.JQueryUtilsMergeTest"></class>
    	</classes>
