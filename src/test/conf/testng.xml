          <class name="org.got5.tapestry5.jquery.MaskTest"></class>
          <class name="org.got5.tapestry5.jquery.RevealTest"></class>
          <class name="org.got5.tapestry5.jquery.SelectorTest"></class>
          <class name="org.got5.tapestry5.jquery.SliderTest"></class>
          <class name="org.got5.tapestry5.jquery.SuperfishTest"></class>
		  <class name="org.got5.tapestry5.jquery.TooltipTest"></class>
          <class name="org.got5.tapestry5.jquery.ValidationTest"></class> 
          <class name="org.got5.tapestry5.jquery.AutocompleteTest"></class>
