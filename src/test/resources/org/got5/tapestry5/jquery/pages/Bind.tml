<html t:type="layout" 
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" 
      xmlns:p="tapestry:parameter"
      xmlns:j="tapestry-library:jquery">

    <t:zone t:id="clickZone" t:visible="false">
        click OK
    </t:zone>

    <div t:id="clickHere" 
         t:type="any"
         t:mixins="jquery/bind" 
         t:eventType="click"
         t:event="click"
         t:zone="prop:clickzone.clientid">
        click test
    </div>
</html>