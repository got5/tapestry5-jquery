<html t:type="layoutWithAccordion" 
	  t:activeElement="literal:2"
	  t:title="ZoneRefresh"
	  xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd" 
	  xmlns:p="tapestry:parameter">
    
     	
    <t:jquery.tabs  t:listTabData="listTabData" t:activePanelId="0" />
    
    <t:block id="docs">
    	<t:componentDocumentation>
			<p:title>
				Zone Refresh
			</p:title>
			<p:description>
				This mixin allows you to update a zone with a period. This mixin use the title attribute of your element
			</p:description>
			
			<p:link_1><t:componentLink t:url="http://access.aol.com/csun2011"/></p:link_1>
		</t:componentDocumentation>
	</t:block>
    
    <t:block id="example">
    	<div t:type='zone' t:mixins='jquery/zonerefresh' t:period='1'>${now} </div>
 		<div t:type='zone' id="zone2" t:mixins='jquery/zonerefresh' t:period='1'>${now} </div>
    	<br />
    	<br />
