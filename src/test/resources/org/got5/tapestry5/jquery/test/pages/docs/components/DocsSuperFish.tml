	<t:jquery.tabs  t:listTabData="prop:listTabData" t:activePanelId="0" />
    
    <t:block id="docs">
    	<t:componentDocumentation>
			<p:title>
				Superfish Component
			</p:title>
			<p:description>
				The Component allows you to create a CSS drop-down menu
			</p:description>
			
			<p:link_1><t:componentLink t:url="http://users.tpg.com.au/j_birch/plugins/superfish/"/></p:link_1>
			
			<p:param_1>
				<t:componentParameter
					t:name="params"
					t:required="false"
					t:javaType="JSONObject"
					t:prefix="prop"
					t:description="JSON Object for the Superfish Configuration"
				/>
			</p:param_1>
			<p:param_2>
				<t:componentParameter
					t:name="vertical"
					t:required="false"
					t:javaType="Boolean"
					t:prefix="literal"
					t:description="your menu will be vertical (vertical=true) or your menu will have the navbar stylesheet (vertical=false)"
				/>
			</p:param_2>
			<p:param_3>
				<t:componentParameter
					t:name="classe  "
					t:required="false"
					t:javaType="String"
					t:prefix="literal"
					t:defaultValue="sf-menu"
					t:description="CSS class for the menu. By default sf-menu"
				/>
			</p:param_3>
			<p:param_4>
				<t:componentParameter
					t:name="supersubs  "
					t:required="false"
					t:javaType="JSONObject"
					t:prefix="prop"
					t:description="Use the supersubs plugin"
				/>
			</p:param_4>
			<p:param_5>
				<t:componentParameter
					t:name="supersubsParams  "
					t:required="false"
					t:javaType="JSONObject"
					t:prefix="prop"
					t:description="JSON Object for the Supersubs configuration"
				/>
			</p:param_5>
			
		</t:componentDocumentation>
    </t:block>
    
